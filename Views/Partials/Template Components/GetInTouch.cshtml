@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
 <!-- Get in touch Start -->
   <div class="get-in-touch bg-blue">
      <div class="container">
         <div class="row pt-10 pb-10">
            <div class="border-design"></div>
            <div class="col-md-6 d-flex align-items-center">
               <div class="ct-inr-block">
                  <h2>Get in touch</h2>
                  <p>For more information on how NWEH<br> can assist you with your trial<br> please contact us</p>
               </div>
            </div>
            <div class="col-md-6">
               <div class="ct-form">
                  <div class="form-group">
                     <input type="text" placeholder="Full name*" class="form-control">
                  </div>
                  <div class="form-group">
                     <input type="text" placeholder="Company email address*" class="form-control">
                  </div>
                  <div class="form-group">
                     <input type="text" placeholder="Company name*" class="form-control">
                  </div>
                  <div class="form-group">
                     <textarea class="form-control" placeholder="Message*"></textarea>
                  </div>
                  <div class="form-group text-center m-0">
                     <button type="submit" class="btn btn-custom">Submit</button>
                  </div>
               </div>
            </div>
         </div>
      </div>
   </div>
   <!-- Get in touch End -->
